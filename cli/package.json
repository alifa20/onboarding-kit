{
  "name": "onboardkit",
  "version": "1.0.0",
  "description": "Zero-cost AI-powered onboarding screen generator for React Native/Expo",
  "type": "module",
  "bin": {
    "onboardkit": "./dist/index.js"
  },
  "files": [
    "dist",
    "templates",
    "examples",
    "docs"
  ],
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest --run src/__tests__/integration",
    "test:platform": "vitest --run src/__tests__/platform",
    "test:examples": "vitest --run src/__tests__/examples",
    "test:ci": "vitest --run --coverage",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "react-native",
    "expo",
    "onboarding",
    "cli",
    "code-generator",
    "ai",
    "oauth",
    "react-navigation",
    "typescript",
    "mobile",
    "app-development"
  ],
  "author": "Ali",
  "license": "MIT",
  "homepage": "https://github.com/alifa20/onboarding-kit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/alifa20/onboarding-kit.git"
  },
  "bugs": {
    "url": "https://github.com/alifa20/onboarding-kit/issues"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "@clack/prompts": "^1.0.0",
    "unified": "^11.0.5",
    "remark-parse": "^11.0.0",
    "remark-frontmatter": "^5.0.0",
    "zod": "^3.24.1",
    "handlebars": "^4.7.8",
    "prettier": "^3.4.2",
    "picocolors": "^1.1.1",
    "@anthropic-ai/sdk": "^0.32.0",
    "@google/generative-ai": "^0.22.0",
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.3",
    "@types/node": "^22.10.5",
    "vitest": "^2.1.8",
    "@vitest/coverage-v8": "^2.1.8"
  }
}
